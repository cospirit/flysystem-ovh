{
    "name": "ardeveloppement/flysystem-ovh",
    "description": "Flysystem Adapter for OVH Openstack Swift Object Storage",
    "homepage": "https://github.com/ArDeveloppement/flysystem-ovh.git",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nuwan Sumihiran Bandara",
            "email": "nuwan@techyah.com"
        },
        {
            "name": "ArDeveloppement",
            "email": "contact@ardeveloppement.fr"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "~1.0",
        "rackspace/php-opencloud": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "ArDev\\Flysystem\\OVH\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
